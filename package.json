{
	"name": "doxsite",
	"version": "1.0.0",
	"description": "Utility for creating highly customised documentation websites with XML generated by Doxygen.",
	"type": "module",
	"main": "./src/doxsite.js",
	"scripts": {
		"create-project": "doxsite -n Documentation",
		"run-build": "node src/runbuild.js",
		"run-doxygen": "node src/rundoxygen.js",
		"build-docs": "node src/builddocs.js"
	},
	"bin": {
		"doxsite": "src/cli/cli.js"
	},
	"keywords": [
		"doxygen",
		"doc",
		"docs",
		"documentation",
		"generator",
		"generation",
		"code"
	],
	"author": "Geoffrey Walker",
	"repository": {
		"type": "git",
		"url": "https://github.com/wondeluxe/doxsite"
	},
	"license": "ISC",
	"funding": [{
		"type": "paypal",
		"url": "https://paypal.me/wondeluxe"
	}],
	"dependencies": {
		"fast-xml-parser": "^4.1.3"
	}
}
